{
   "author": "Paul THEIS",
   "background": {
      "page": "/background/html/background.html"
   },
   "browser_action": {
      "default_icon": "/global/images/icons/128.png",
      "default_popup": "/popup/html/popup.html",
      "default_title": "Facebook Toolkit"
   },
   "content_scripts": [ {
      "js": [ "/background/js/hideseen.js" ],
      "matches": [ "https://*.facebook.com/*", "*://*.facebook.com/*" ]
   } ],
   "description": "(Facebook Toolkit) Collection of tools for Facebook.com.",
   "icons": {
      "128": "/global/images/icons/128.png",
      "16": "/global/images/icons/16.png",
      "32": "/global/images/icons/32.png",
      "48": "/global/images/icons/48.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "Toolkit For Facebook",
   "permissions": [ "storage", "*://ssl.google-analytics.com/*", "*://*.facebook.com/*", "*://*.appspot.com/*", "notifications", "tabs", "clipboardWrite" ],
   "short_name": "FTK",
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.0.5",
   "web_accessible_resources": [ "content_old/*", "content_new/*", "images/*", "global/css/*" ]
}
